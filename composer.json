{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.4.x-dev",
        "jenssegers/mongodb": "^3.0",
        "predis/predis": "^1.0",
        "laravelcollective/html": "5.3.*",
        "omnipay/paypal": "*",
        "nazka/sermepa-omnipay": "dev-master",
        "creitive/laravel5-breadcrumbs": "^2.0",
        "intervention/image": "^2.3",
        "spatie/laravel-cookie-consent": "^1.2",
        "torann/laravel-meta-tags": "^3.0",
        "ignited/laravel-omnipay": "^2.1",
        "awjudd/feed-reader": "1.3.*",
        "darryldecode/cart": "dev-master",
        "appzcoder/crud-generator": "^1.1",
        "hoa/math": "1.16.08.29",
        "rinvex/repository": "^2.0",
        "pixelpeter/laravel5-woocommerce-api-client": "^2.0",
        "pulkitjalan/geoip": "2.*",
        "martinlindhe/laravel-vue-i18n-generator": "^0.1.7",
        "thujohn/twitter": "^2.2",
        "sentry/sentry-laravel": "^0.5",
        "graham-campbell/htmlmin": "^4.4",
        "ebisbe/fake-omnipay": "dev-master",
        "laravel/tinker": "~1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "barryvdh/laravel-debugbar": "^2.2",
        "barryvdh/laravel-ide-helper": "^2.2"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Tests\\": "tests/"
        },
        "files": [
            "app/Business/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan vue-i18n:generate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ebisbe/laravelshoppingcart"
        }
    ]
}